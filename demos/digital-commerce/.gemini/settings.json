{
  "general": {
    "checkpointing": {
      "enabled": true
    },
    "preferredEditor": "code",
    "vimMode": {
        "enabled": false
    }
  },
  "contexts": {
    "fileName": ["GEMINI.md", "AGENT.md"]
  },
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    }
  },
  "telemetry": {
    "enabled": true,
    "target": "gcp",
    "logPrompts": true
  },
  "privacy": {
    "usageStatisticsEnabled": true
  },
  "model": {
    "name": "gemini-2.5-pro"
  },
  "ui": {
    "theme": "GitHub",
    "hideBanner": false,
    "hideTips": false,
    "showCitations": false
  }
}